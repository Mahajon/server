{
    "builds": [
        {
            "src": "mahajon/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build-files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles_builds" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mahajon/wsgi.py"
        }
    ]
}
