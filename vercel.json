{
    "builds": [
        {
            "src": "mahajon/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build-files.sh",
            "use": "@vercel/static-build",
            "config": { "distDir": "staticfiles" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "mahajon/wsgi.py"
        }
    ],
    "outputDirectory": "staticfiles"
}
